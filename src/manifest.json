{
    "description": "Open current tab or selected link in Incognito Mode",
    "icons": {
        "128": "img/icon.png",
        "64": "img/icon.png",
        "32": "img/icon.png",
        "20": "img/icon.png",
        "18": "img/icon.png",
        "16": "img/icon.png"
    },
    "manifest_version": 2,
    "name": "Open in Incognito",
    "version": "1.0.1",
    "permissions": [
        "contextMenus",
        "tabs"
    ],
    "incognito": "spanning",
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}